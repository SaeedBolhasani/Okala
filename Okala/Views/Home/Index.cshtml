@{
    ViewData["Title"] = "Home Page";
}
<style>
    table {
        width: 100%;
        border-collapse: collapse;
    }

    th, td {
        border: 1px solid #ddd;
        padding: 8px;
    }

    th {
        background-color: #f2f2f2;
    }

    tr:hover {
        background-color: #f5f5f5;
    }
</style>
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    <table>
        <tr>
            <th>Timestamp</th>
            <th>Error Code</th>
            <th>Error Message</th>
            <th>Elapsed</th>
            <th>Credit Count</th>
            <th>Notice</th>
        </tr>
        <tr>
            <td>@Model.Status.timestamp</td>
            <td>@Model.Status.error_code</td>
            <td>@Model.Status.error_message</td>
            <td>@Model.Status.elapsed</td>
            <td>@Model.Status.credit_count</td>
            <td>@Model.Status.notice</td>
        </tr>
    </table>
    <h2>Data</h2>
    <table>
        <tr>
            <th>Name</th>
            <th>Symbol</th>
            <th>Description</th>
            <th>Quotes</th>
        </tr> @foreach (var entry in Model.Data)
        {
            foreach (var symbolInfo in entry.Value)
            {
                <tr>
                    <td>@symbolInfo.Name</td>
                    <td>@symbolInfo.Symbol</td>
                    <td>@symbolInfo.Description</td>
                    <td>
                        @foreach (var quote in symbolInfo.Quote)
                        {
                            <div>@quote.Key: @quote.Value.Price</div>
                        }
                    </td>
                </tr>
            }
        }
    </table>
</div>
